{
	"name": "discord-embed-editor",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"smui-theme-light": "smui-theme compile static/smui.css -i src/theme -i ./node_modules ../../node_modules",
		"smui-theme-dark": "smui-theme compile static/smui-dark.css -i src/theme/dark -i ./node_modules ../../node_modules",
		"build": "vite build",
		"preview": "vite preview",
		"test": "npm run test:integration && npm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest",
		"prepare": " pnpm run smui-theme-light && pnpm run smui-theme-dark"
	},
	"devDependencies": {
		"@material/theme": "^14.0.0",
		"@playwright/test": "^1.49.1",
		"@sveltejs/adapter-auto": "^3.3.1",
		"@sveltejs/adapter-cloudflare": "^5.0.0",
		"@sveltejs/kit": "^2.15.1",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"@typescript-eslint/eslint-plugin": "^8.19.0",
		"@typescript-eslint/parser": "^8.19.0",
		"discord-api-types": "^0.37.114",
		"discord-markdown-parser": "file:subtree\\discord-markdown-parser",
		"eslint": "^9.17.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.46.1",
		"highlight.js": "^11.11.1",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.2",
		"smui-theme": "8.0.0-beta.3",
		"svelte": "^5.16.0",
		"svelte-check": "^4.1.1",
		"tslib": "^2.8.1",
		"typescript": "^5.7.2",
		"vite": "^6.0.6",
		"vite-plugin-dynamic-import": "^1.6.0",
		"vitest": "^2.1.8",
		"wrangler": "^3.99.0"
	},
	"type": "module",
	"dependencies": {
		"@discordjs/rest": "^2.4.0",
		"@smui/button": "8.0.0-beta.3",
		"@smui/card": "8.0.0-beta.3",
		"@smui/checkbox": "8.0.0-beta.3",
		"@smui/circular-progress": "8.0.0-beta.3",
		"@smui/common": "8.0.0-beta.3",
		"@smui/data-table": "8.0.0-beta.3",
		"@smui/dialog": "8.0.0-beta.3",
		"@smui/form-field": "8.0.0-beta.3",
		"@smui/icon-button": "8.0.0-beta.3",
		"@smui/segmented-button": "8.0.0-beta.3",
		"@smui/snackbar": "8.0.0-beta.3",
		"@smui/tab": "8.0.0-beta.3",
		"@smui/tab-bar": "8.0.0-beta.3",
		"@smui/textfield": "8.0.0-beta.3",
		"moment": "^2.30.1",
		"svelte-awesome-color-picker": "^3.1.4",
		"svelte-material-ui": "8.0.0-beta.3",
		"twemoji": "^14.0.2"
	}
}